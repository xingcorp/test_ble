<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup disableIfNoEncryptionCapabilities="true">
        <!-- Exclude sensitive beacon data -->
        <exclude domain="sharedpref" path="nordic_beacon_encrypted_prefs.xml"/>
        <exclude domain="database" path="nordic_beacon_database"/>
    </cloud-backup>
    
    <device-transfer>
        <!-- Include only configuration data -->
        <include domain="sharedpref" path="app_preferences.xml"/>
    </device-transfer>
</data-extraction-rules>
